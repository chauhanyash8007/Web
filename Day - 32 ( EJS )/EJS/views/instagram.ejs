<%- include("includes/head.ejs")%>
<!-- don't need / before includes it will give an error -->
  <body>
    <h2>This page belongs to @<%= Data.name%></h2>
    <button>Follow</button>
    <button>Message</button>
    <p>
      Followers : <%= Data.followers%> &nbsp;&nbsp;&nbsp;&nbsp;Following : <%=
      Data.following%>
    </p>
    <hr />
    <%for(let post of Data.posts){%>
    <img src="<%= post.image %>" alt="some image" />
    <p>
      Likes : <%= post.likes%> &nbsp;&nbsp;&nbsp;&nbsp;Comments : <%=
      post.comments%>
    </p>
    <%}%>
    <%- include("includes/footer.ejs")%>
  </body>
</html>
